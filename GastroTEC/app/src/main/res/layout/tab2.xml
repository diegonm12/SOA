<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/comida_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="150dp"
        android:gravity="center"
        android:text=""
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView1_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="45dp"
        android:text="TextView"
        android:textSize="14dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="0dp"
        />

    <TextView
        android:id="@+id/textView2_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/textView1_horario_comidas"
        android:layout_centerHorizontal="true"
        android:text="TextView"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/textView3_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView1_horario_comidas"
        android:layout_marginEnd="5dp"
        android:text="TextView"
        android:textSize="14dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="0dp"
        />

    <ImageView
        android:id="@+id/imageView_horario_comidas"
        android:layout_width="65dp"
        android:layout_height="58dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="193dp"
        android:layout_toEndOf="@+id/textView1_horario_comidas"
        android:contentDescription="like"
        android:src="@drawable/corazon"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/imageView2_horario_comidas"
        android:layout_width="67dp"
        android:layout_height="51dp"
        android:layout_alignBottom="@+id/imageView_horario_comidas"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="109dp"
        android:layout_marginRight="109dp"
        android:contentDescription="dislike"
        android:visibility="invisible"
        android:src="@drawable/nolike" />

    <TextView
        android:id="@+id/textView8_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView_horario_comidas"
        android:layout_toEndOf="@+id/textView1_horario_comidas"
        android:text="" />

    <TextView
        android:id="@+id/textView9_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageView2_horario_comidas"
        android:layout_alignTop="@+id/textView8_horario_comidas"
        android:text=""
        android:layout_alignRight="@+id/imageView2_horario_comidas" />
</RelativeLayout>