<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView
        android:id="@+id/comida_horario_comidas"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical|center_horizontal"
        android:text=""
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView1_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="60dp"
        android:text="TextView"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="0dp"
        />

    <TextView
        android:id="@+id/textView2_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView1_horario_comidas"
        android:layout_centerHorizontal="true"
        android:text="TextView"
        />

    <TextView
        android:id="@+id/textView3_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView1_horario_comidas"
        android:layout_marginEnd="5dp"
        android:text="TextView"
        android:layout_alignParentRight="true"
        android:layout_marginRight="0dp"
        />

    <ImageView
        android:id="@+id/imageView_horario_comidas"
        android:layout_width="65dp"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="129dp"
        android:layout_toEndOf="@+id/textView1_horario_comidas"
        android:layout_toRightOf="@+id/textView1_horario_comidas"
        android:contentDescription="like"
        android:visibility="visible"
        android:src="@drawable/corazon" />

    <ImageView
        android:id="@+id/imageView2_horario_comidas"
        android:layout_width="67dp"
        android:layout_height="51dp"
        android:layout_alignBottom="@+id/imageView_horario_comidas"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="109dp"
        android:layout_marginRight="109dp"
        android:contentDescription="dislike"
        android:visibility="visible"
        android:src="@drawable/nolike" />

    <TextView
        android:id="@+id/textView8_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="84dp"
        android:layout_toEndOf="@+id/textView1_horario_comidas"
        android:text="TextView"
        android:layout_toRightOf="@+id/textView1_horario_comidas" />

    <TextView
        android:id="@+id/textView9_horario_comidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView2_horario_comidas"
        android:layout_alignTop="@+id/textView8_horario_comidas"
        android:text="TextView"
        android:layout_alignLeft="@+id/imageView2_horario_comidas" />
</RelativeLayout>